} md={searchScreen ? 4 : 6} className=\"videoHorizontal__left\">\n        <LazyLoadImage\n          className={`videoHorizontal__thumbnail ${thumbnail}`}\n          wrapperClassName=\"videoHorizontal__thumbnail-wrapper\"\n          src={medium.url}\n          alt=\"썸네일\"\n          effect=\"blur\"\n        />\n        {isVideo && (\n          <span className=\"videoHorizontal__duration\">{_duration}</span>\n        )}\n      </Col>\n\n      <Col\n        className=\"videoHorizontal__right p-0\"\n        xs={6}\n        md={searchScreen ? 6 : 6}\n      >\n        <p className=\"videoHorizontal__title mb-1\">{title}</p>\n\n        {isVideo && (\n          <div className=\"videoHorizontal_details\">\n            <AiFillEye /> {numeral(views).format(\"0.a\")} Views •\n            {moment(publishedAt).fromNow()}\n          </div>\n        )}\n\n        {searchScreen && <p className=\"mt-1\"> {description}</p>}\n\n        <div className=\"videoHorizontal__channel d-flex align-items-center my-1\">\n          {isVideo && <LazyLoadImage src={channelIcon?.url} effect=\"blur \" />}\n          <p className=\"mb-0\">{channelTitle}</p>\n        </div>\n      </Col>\n    </Row>\n  );\n}\nexport default VideoHorizontal;\n","/Users/pokeoseu/Desktop/youtue-clone/src/components/comments/Comments.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/components/comment/Comment.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/redux/reducers/channel.reducer.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/redux/actions/channel.action.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/redux/actions/comments.action.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/redux/reducers/comments.reducer.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/screens/SearchScreen/SearchScreen.js",[],{"ruleId":"127","replacedBy":"128"},{"ruleId":"129","replacedBy":"130"},{"ruleId":"131","severity":1,"message":"132","line":1,"column":17,"nodeType":"133","messageId":"134","endLine":1,"endColumn":26},{"ruleId":"131","