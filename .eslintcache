uter-dom\";\nimport { MdDescription } from \"react-icons/md\";\n\n//using react-bootstarp\nfunction VideoHorizontal({ video, searchScreen }) {\n  const [views, setViews] = useState(null);\n  const [duration, setDuration] = useState(null);\n  const [channelIcon, setChannelIcon] = useState(null);\n\n  const {\n    id,\n    snippet: {\n      channelId,\n      channelTitle,\n      title,\n      description,\n      publishedAt,\n      thumbnails: { medium },\n    },\n  } = video;\n\n  const isVideo = id.kind === \"youtube#video\";\n  const thumbnail = !isVideo && `videoHorizontal__thumbnail-channel`;\n\n  useEffect(() => {\n    const get_video_details = async () => {\n      const {\n        data: { items },\n      } = await request(\"/videos\", {\n        params: {\n          part: \"contentDetails,statistics\",\n          id: id.videoId,\n        },\n      });\n\n      setDuration(items[0].contentDetails.duration);\n      setViews(items[0].statistics.viewCount);\n      console.log(\"videoDetail\", items[0]);\n    };\n    get_video_details();\n  }, [id]);\n\n  useEffect(() => {\n    const get_channel_icon = async () => {\n      const {\n        data: { items },\n      } = await request(\"/channels\", {\n        params: {\n          part: \"snippet\",\n          id: channelId,\n        },\n      });\n      console.log(items);\n      setChannelIcon(items[0].snippet.thumbnails.default);\n    };\n\n    get_channel_icon();\n  }, [channelId]);\n\n  const seconds = moment.duration(duration).asSeconds();\n  const _duration = moment.utc(seconds * 1000).format(\"mm:ss\");\n\n  const history = useHistory();\n\n  const handleClick = () => {\n    isVideo\n      ? history.push(`/watch/${id.videoId}`)\n      : history.push(`/cahnnel/${id.channelId}`);\n  };\n  return (\n    <Row\n      onClick={handleClick}\n      className=\"videoHorizontal m-1 py-2 align-items-center\"\n    >\n      {/* //TODO refracter grid  */}\n      <Col xs={6} md={searchScreen ? 4 : 6} className=\"videoHorizontal__left\">\n        <LazyLoadImage\n          className={`videoHorizontal__thumbnail ${thumbnail}`}\n          wrapperClassName=\"videoHorizontal__thumbnail-wrapper\"\n          src={medium.url}\n          alt=\"썸네일\"\n          effect=\"blur\"\n        />\n        {isVideo && (\n          <span className=\"videoHorizontal__duration\">{_duration}</span>\n        )}\n      </Col>\n\n      <Col\n        className=\"videoHorizontal__right p-0\"\n        xs={6}\n        md={searchScreen ? 6 : 6}\n      >\n        <p className=\"videoHorizontal__title mb-1\">{title}</p>\n\n        {isVideo && (\n          <div className=\"videoHorizontal_details\">\n            <AiFillEye /> {numeral(views).format(\"0.a\")} Views •\n            {moment(publishedAt).fromNow()}\n          </div>\n        )}\n\n        {searchScreen && <p className=\"mt-1\"> {description}</p>}\n\n        <div className=\"videoHorizontal__channel d-flex align-items-center my-1\">\n          {isVideo && <LazyLoadImage src={channelIcon?.url} effect=\"blur \" />}\n          <p className=\"mb-0\">{channelTitle}</p>\n        </div>\n      </Col>\n    </Row>\n  );\n}\nexport default VideoHorizontal;\n","/Users/pokeoseu/Desktop/youtue-clone/src/components/comments/Comments.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/components/comment/Comment.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/redux/reducers/channel.reducer.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/redux/actions/channel.action.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/redux/actions/comments.action.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/redux/reducers/comments.reducer.js",[],"/Users/pokeoseu/Desktop/youtue-clone/src/screens/SearchScreen/SearchScreen.js",[],{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":1,"column":17,"nodeType":"130","messageId":"131","endLine":1,"endColumn":26},{"ruleId":"128","severity":1,"message":"132","line":8,"column":16,"node